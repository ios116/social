[mysqld]
character-set-server = utf8
collation-server = utf8_unicode_ci
skip-character-set-client-handshake
bind-address           = 0.0.0.0

#salve
binlog_do_db = soc_db
server_id = 2
binlog_format = ROW
gtid_mode = on
enforce_gtid_consistency
# только в режиме чтения
read-only=on
#skip_slave_start
# при необходимости создания цепочки
#log_slave_updates
# для слейва необязательно
#log_bin = mysql-bin
